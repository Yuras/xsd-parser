<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:spct="http://services.sabre.com/sp/common/simple/v3" xmlns:stlp="http://services.sabre.com/STL_Payload/v02_01" xmlns:ns1="http://services.sabre.com/sp/air/ticket/v1" targetNamespace="http://services.sabre.com/sp/air/ticket/v1" elementFormDefault="qualified">
  <import namespace="http://services.sabre.com/STL_Payload/v02_01" schemaLocation="built-ins/STL2_Payload_v02_01.xsd"/>
  <import namespace="http://services.sabre.com/sp/common/simple/v3" schemaLocation="SpCommonSimpleTypes_v3.xsd"/>
  <element name="AirTicketRQ">
    <complexType>
      <complexContent>
        <extension base="stlp:STL_Request_Payload">
          <sequence>
            <element name="DesignatePrinter" minOccurs="0">
              <complexType>
                <sequence>
                  <element name="Printers" minOccurs="0">
                    <complexType>
                      <sequence>
                        <element name="BagTag" minOccurs="0">
                          <complexType>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="BoardingPass" minOccurs="0">
                          <complexType>
                            <attribute name="HostedCarrier" type="boolean" use="optional">
                              <annotation>
                                <documentation>"HostedCarrier" is used by airline customers to indicate that this request is for an airline-based boarding pass printer.</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Hardcopy" minOccurs="0">
                          <complexType>
                            <attribute name="Spacing" use="optional">
                              <annotation>
                                <documentation>"Spacing" is used to specify the spacing, either 1, or 2.</documentation>
                              </annotation>
                              <simpleType>
                                <restriction base="string">
                                  <enumeration value="1"/>
                                  <enumeration value="2"/>
                                </restriction>
                              </simpleType>
                            </attribute>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="InvoiceItinerary" minOccurs="0">
                          <complexType>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="ListLabel" minOccurs="0">
                          <complexType>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="MiniItinerary" minOccurs="0">
                          <complexType>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Report" minOccurs="0">
                          <complexType>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Ticket" minOccurs="0">
                          <complexType>
                            <sequence>
                              <element name="MiniItinerary" minOccurs="0">
                                <complexType>
                                  <attribute name="LNIATA" type="string" use="optional">
                                    <annotation>
                                      <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element name="Remote" minOccurs="0">
                                <complexType>
                                  <attribute name="LNIATA" type="string" use="optional">
                                    <annotation>
                                      <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="CountryCode" type="string" use="optional"/>
                            <attribute name="LNIATA" type="string" use="optional">
                              <annotation>
                                <documentation>"LNIATA" is used to specify a bag tag printer address.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element name="Profile" minOccurs="0">
                    <complexType>
                      <attribute name="Number" type="integer" use="optional">
                        <annotation>
                          <documentation>"Number" is used to specify a previously established printer number.</documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element name="Itinerary" minOccurs="1">
              <complexType>
                <attribute name="ID" use="required">
                  <annotation>
                    <documentation>"ID" is used to specify a record locator.</documentation>
                  </annotation>
                  <simpleType>
                    <restriction base="string"/>
                  </simpleType>
                </attribute>
              </complexType>
            </element>
            <element name="AccountingLines" minOccurs="0">
              <complexType>
                <sequence>
                  <element name="Delete" minOccurs="0" maxOccurs="unbounded">
                    <complexType>
                      <attribute name="EndNumber" type="integer" use="optional">
                        <annotation>
                          <documentation>"EndNumber" when used with @Number to specify a range of accounting lines to delete.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Number" type="integer" use="required">
                        <annotation>
                          <documentation>"Number" is used to specify particular accounting lines.</documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="All" type="boolean" use="optional">
                  <annotation>
                    <documentation>"All" is used to delete all accounting lines, cannot combine with attribute None.</documentation>
                  </annotation>
                </attribute>
                <attribute name="None" type="boolean" use="optional">
                  <annotation>
                    <documentation>"None" is used to indicate not to delete any of the accounting lines, cannot combine with "All" attribute.</documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
            <element name="Ticketing" minOccurs="1" maxOccurs="99">
              <complexType>
                <sequence>
                  <element name="FlightQualifiers" minOccurs="0">
                    <complexType>
                      <sequence>
                        <element name="VendorPrefs" minOccurs="0">
                          <complexType>
                            <sequence>
                              <element name="Airline">
                                <complexType>
                                  <attribute name="Code" type="string" use="required">
                                    <annotation>
                                      <documentation>"Code" is used to specify a particular vendor code.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element name="FOP_Qualifiers" minOccurs="0">
                    <complexType>
                      <sequence>
                        <element name="BasicFOP" minOccurs="0">
                          <annotation>
                            <documentation>BasicFOP cannot combine with .../BSP_Ticketing, or .../SabreSonicTicketing, or .../Multiple CC_FOP.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="CC_Info" minOccurs="0">
                                <complexType>
                                  <sequence>
                                    <element name="PaymentCard">
                                      <complexType>
                                        <attribute name="Code" type="string" use="optional">
                                          <annotation>
                                            <documentation>"Code" is used to specify a credit card vendor code.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="CardSecurityCode" type="string" use="optional">
                                          <annotation>
                                            <documentation>"CardSecurityCode" is used to specify a credit card Card Security Code number.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="ExpireDate" type="spct:expireDate" use="optional">
                                          <annotation>
                                            <documentation>"ExpireDate" is used to specify a credit card expiration date, follows this format: YYYY-MM.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="ExtendedPayment" type="integer" use="optional">
                                          <annotation>
                                            <documentation>"ExtendedPayment" is used to specify extended payment if applicable.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="ManualApprovalCode" type="string" use="optional">
                                          <annotation>
                                            <documentation>"ManualApprovalCode" is used to specify a manual credit card approval code if applicable.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="Number" type="integer" use="optional">
                                          <annotation>
                                            <documentation>"Number" is used to specify a credit card number.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                  </sequence>
                                  <attribute name="Suppress" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"Suppress" is used to specify whether or not to suppress credit card information on an itinerary.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="Reference" type="integer" use="optional"/>
                            <attribute name="Type" type="string" use="optional">
                              <annotation>
                                <documentation>"Type" is used to specify the payment type.  Acceptable values are "CA," (cash) "CK," (check) "CQ," (cheque).  If utilizing a credit card please omit @Type, and populate the .../CC_Info child node.</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="Virtual" type="string" use="optional"/>
                          </complexType>
                        </element>
                        <element name="BSP_Ticketing" minOccurs="0">
                          <complexType>
                            <sequence>
                              <element name="MultipleFOP" minOccurs="0">
                                <annotation>
                                  <documentation>MultipleFOP cannot combine with .../MultipleMiscFOP, or .../PayLaterPlan</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="Fare">
                                      <complexType>
                                        <attribute name="Amount" type="string" use="required">
                                          <annotation>
                                            <documentation>"Amount" is used to specify the amount to be applied to the second FOP.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="FOP_One">
                                      <complexType>
                                        <sequence>
                                          <element name="CC_Info" minOccurs="0">
                                            <complexType>
                                              <sequence>
                                                <element name="PaymentCard">
                                                  <complexType>
                                                    <attribute name="Code" type="string" use="required">
                                                      <annotation>
                                                        <documentation>"Code" is used to specify the credit card vendor code.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="CardSecurityCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"CardSecurityCode" is used to specify a credit card Card Security Code number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                      <annotation>
                                                        <documentation>"ExpireDate" is used to specify the credit card expiration date, follows this format: YYYY-MM.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExtendedPayment" type="integer" use="optional">
                                                      <annotation>
                                                        <documentation>"ExtendedPayment" is used to specify extended payment if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualApprovalCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"ManualApprovalCode" is used to specify a manual credit card approval code if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualOBFee" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"ManualOBFee" is used to indicate that the manual OB fee should be associated to the card. A manual OB fee can ONLY be added to a ticketing entry that references a PQ that has the PRICE-MANUAL tag at the bottom of the *PQ</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="Number" type="integer" use="required">
                                                      <annotation>
                                                        <documentation>"Number" is used to specify the credit card number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                  </complexType>
                                                </element>
                                              </sequence>
                                              <attribute name="Suppress" type="boolean" use="optional">
                                                <annotation>
                                                  <documentation>"Suppress" is used to specify whether or not to suppress credit card information on an itinerary.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Type" type="string" use="optional">
                                          <annotation>
                                            <documentation>"Type" is used to specify the payment type.  Acceptable values are "CA,"CK," or "CQ."  If utilizing a credit card please omit @Type, and populate the .../CC_Info child node.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="FOP_Two">
                                      <complexType>
                                        <sequence>
                                          <element name="CC_Info" minOccurs="0">
                                            <complexType>
                                              <sequence>
                                                <element name="PaymentCard">
                                                  <complexType>
                                                    <attribute name="Code" type="string" use="required">
                                                      <annotation>
                                                        <documentation>"Code" is used to specify the credit card vendor code.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="CardSecurityCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"CardSecurityCode" is used to specify a credit card Card Security Code number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                      <annotation>
                                                        <documentation>"ExpireDate" is used to specify the credit card expiration date, follows this format: YYYY-MM.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExtendedPayment" type="integer" use="optional">
                                                      <annotation>
                                                        <documentation>"ExtendedPayment" is used to specify extended payment if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualApprovalCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"ManualApprovalCode" is used to specify a manual credit card approval code if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualOBFee" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>b"ManualOBFee" is used to indicate that the manual OB fee should be associated to the card. A manual OB fee can ONLY be added to a ticketing entry that references a PQ that has the PRICE-MANUAL tag at the bottom of the *PQ</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="Number" type="integer" use="required">
                                                      <annotation>
                                                        <documentation>"Number" is used to specify the credit card number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                  </complexType>
                                                </element>
                                              </sequence>
                                              <attribute name="Suppress" type="boolean" use="optional">
                                                <annotation>
                                                  <documentation>"Suppress" is used to specify whether or not to suppress credit card information on an itinerary.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Type" type="string" use="optional">
                                          <annotation>
                                            <documentation>"Type" is used to specify the payment type.  Acceptable values are "CA,"CK," or "CQ."  If utilizing a credit card please omit @Type, and populate the .../CC_Info child node.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="Taxes" minOccurs="0">
                                      <complexType>
                                        <sequence>
                                          <element name="Tax" maxOccurs="3">
                                            <complexType>
                                              <attribute name="Amount" type="string" use="required">
                                                <annotation>
                                                  <documentation>"Amount" is used to specify the tax amount.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="TaxCode" type="string" use="required">
                                                <annotation>
                                                  <documentation>"TaxCode" is used to specify the tax code.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                      </complexType>
                                    </element>
                                  </sequence>
                                </complexType>
                              </element>
                              <element name="MultipleMiscFOP" minOccurs="0">
                                <annotation>
                                  <documentation>MultipleMiscFOP cannot combine with .../MultipleFOP, or .../PayLaterPlan.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="Fare">
                                      <complexType>
                                        <attribute name="Amount" type="string" use="required">
                                          <annotation>
                                            <documentation>"Amount" is used to specify the fare amount to be applied to the second FOP.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="FOP_One">
                                      <complexType>
                                        <sequence>
                                          <element name="CC_Info" minOccurs="0">
                                            <complexType>
                                              <sequence>
                                                <element name="PaymentCard">
                                                  <complexType>
                                                    <attribute name="Code" type="string" use="required">
                                                      <annotation>
                                                        <documentation>"Code" is used to specify the credit card vendor code.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                      <annotation>
                                                        <documentation>"ExpireDate" is used to specify the credit card expiration date, follows this format: YYYY-MM.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExtendedPayment" type="integer" use="optional">
                                                      <annotation>
                                                        <documentation>"ExtendedPayment" is used to specify extended payment if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualApprovalCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"ManualApprovalCode" is used to specify a manual credit card approval code if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="Number" type="integer" use="required">
                                                      <annotation>
                                                        <documentation>"Number" is used to specify the credit card number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                  </complexType>
                                                </element>
                                              </sequence>
                                              <attribute name="Suppress" type="boolean" use="optional">
                                                <annotation>
                                                  <documentation>"Suppress" is used to specify whether or not to suppress credit card information on an itinerary.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Type" type="string" use="optional">
                                          <annotation>
                                            <documentation>"Type" is used to specify the payment type.  Acceptable values are "CA,"CK," or "CQ."  If utilizing a credit card please omit @Type, and populate the .../CC_Info child node.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="FOP_Two">
                                      <complexType>
                                        <sequence>
                                          <element name="ExtendedPayment" minOccurs="0">
                                            <complexType>
                                              <attribute name="NumMonths" type="integer" use="required"/>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Type" type="string" use="required"/>
                                      </complexType>
                                    </element>
                                    <element name="Taxes" minOccurs="0">
                                      <complexType>
                                        <sequence>
                                          <element name="Tax" maxOccurs="2">
                                            <complexType>
                                              <attribute name="Amount" type="string" use="required">
                                                <annotation>
                                                  <documentation>"Amount" is used to specify the tax amount.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="TaxCode" type="string" use="required">
                                                <annotation>
                                                  <documentation>"TaxCode" is used to specify the tax code.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                      </complexType>
                                    </element>
                                  </sequence>
                                </complexType>
                              </element>
                              <element name="PayLaterPlan" minOccurs="0">
                                <annotation>
                                  <documentation>"PayLaterPlan" is only applicable to Brazil, cannot combine with MultipleFOP, or MultipleMiscFOP.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="Fare">
                                      <complexType>
                                        <attribute name="Amount" type="string" use="required">
                                          <annotation>
                                            <documentation>"Amount" is used to specify the remaining fare amount.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="FOP">
                                      <complexType>
                                        <sequence>
                                          <element name="CC_Info" minOccurs="0">
                                            <complexType>
                                              <sequence>
                                                <element name="PaymentCard">
                                                  <complexType>
                                                    <attribute name="Code" type="string" use="required">
                                                      <annotation>
                                                        <documentation>"Code" is used to specify the credit card vendor code.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                      <annotation>
                                                        <documentation>"ExpireDate" is used to specify the credit card expiration date, follows this format: YYYY-MM</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="ManualApprovalCode" type="string" use="optional">
                                                      <annotation>
                                                        <documentation>"ManualApprovalCode" is used to specify a manual credit card approval code if applicable.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                    <attribute name="Number" type="integer" use="required">
                                                      <annotation>
                                                        <documentation>"Number" is used to specify the credit card number.</documentation>
                                                      </annotation>
                                                    </attribute>
                                                  </complexType>
                                                </element>
                                              </sequence>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Type" type="string" use="optional">
                                          <annotation>
                                            <documentation>"Type" is used to specify the payment type.  Acceptable values are "CA,"CK," or "CQ."  If utilizing a credit card please omit @Type, and populate the .../CC_Info child node.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element name="Installment">
                                      <complexType>
                                        <attribute name="Count" type="string" use="required">
                                          <annotation>
                                            <documentation>"Count" is used to specify the number of installments.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="PayLaterReferenceNumber" type="string" use="required">
                                          <annotation>
                                            <documentation>"PayLaterReferenceNumber" is used to specify the pay later reference number.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="Value" type="string" use="required">
                                          <annotation>
                                            <documentation>"Value" is used to specify the value of the installments.  Please note that decimals are not permitted.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                  </sequence>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element name="MultipleCC_FOP" minOccurs="0">
                          <annotation>
                            <documentation>Multiple CC_FOP cannot combine with .../BSP_Ticketing, or .../SabreSonicTicketing</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="Fare">
                                <complexType>
                                  <attribute name="Amount" type="string" use="required">
                                    <annotation>
                                      <documentation>"Amount" is used to specify the amount to be applied to the second CC.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element name="CC_One">
                                <complexType>
                                  <sequence>
                                    <element name="CC_Info" minOccurs="0">
                                      <complexType>
                                        <sequence>
                                          <element name="PaymentCard">
                                            <complexType>
                                              <attribute name="Code" type="string" use="required">
                                                <annotation>
                                                  <documentation>"Code" is used to specify the credit card vendor code.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="CardSecurityCode" type="string" use="optional">
                                                <annotation>
                                                  <documentation>"CardSecurityCode" is used to specify a credit card Card Security Code number.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                <annotation>
                                                  <documentation>"ExpireDate" is used to specify the credit card expiration date, follows this format: YYYY-MM</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ExtendedPayment" type="integer" use="optional">
                                                <annotation>
                                                  <documentation>"ExtendedPayment" is used to specify extended payment if applicable.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ManualApprovalCode" type="string" use="optional">
                                                <annotation>
                                                  <documentation>"ManualApprovalCode" is used to specify a manual credit card if applicable.</documentation>
                                                </annotation>
                                              </attribute>
