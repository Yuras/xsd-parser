                                  <attribute name="EndNumber" type="integer" use="optional"/>
                                  <attribute name="Number" type="integer" use="required"/>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element name="NameSelect" minOccurs="0" maxOccurs="unbounded">
                          <annotation>
                            <documentation>"NameSelect" is used to instruct the system to price theitinerary based upon a particular name field.</documentation>
                          </annotation>

                          <complexType>
                            <attribute name="EndNameNumber" type="string" use="optional"/>
                            <attribute name="NameNumber" type="string" use="required"/>
                          </complexType>
                        </element>
                        <element name="PhaseIV" minOccurs="0" maxOccurs="7">
                          <annotation>
                            <documentation>Cannot combine with .../PriceQuote.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="NameSelect" minOccurs="0" maxOccurs="unbounded">
                                <annotation>
                                  <documentation>"NameSelect" is used to instruct the system to price theitinerary based upon a particular name field.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="EndNameNumber" type="string" use="optional"/>
                                  <attribute name="NameNumber" type="string" use="required"/>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="Number" type="integer" use="required"/>
                          </complexType>
                        </element>
                        <element name="PriceQuote" minOccurs="0" maxOccurs="unbounded">
                          <annotation>
                            <documentation>Cannot combine with .../NameSelect. Cannot combine with .../PhaseIV.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="NameSelect" minOccurs="0" maxOccurs="unbounded">
                                <annotation>
                                  <documentation>"NameSelect" is used to instruct the system to price theitinerary based upon a particular name field.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="EndNameNumber" type="decimal" use="optional"/>
                                  <attribute name="NameNumber" type="decimal" use="required"/>
                                </complexType>
                              </element>
                              <element name="Record" minOccurs="0" maxOccurs="unbounded">
                                <annotation>
                                  <documentation>In order to specify multiple PQR records to be issued please specify a range or specific record numbers by means of the element repetition. .../Ticketing/PricingQualifiers/PriceQuote/Record/@Reissue cannot be combined with .../Ticketing/MiscQualifiers/Ticket</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="EndNumber" type="integer" use="optional">
                                    <annotation>
                                      <documentation>"EndNumber" is used to specify the ending PQ number in a range.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Number" type="integer" use="required">
                                    <annotation>
                                      <documentation>"Number" is used to specify a PQ number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Reissue" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"Reissue" is used to indicate that the particular PQ record is a PQ Reissue record.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element name="RefundableBalance" minOccurs="0">
                          <complexType>
                            <sequence>
                              <element name="Taxes" minOccurs="0">
                                <complexType>
                                  <sequence>
                                    <element name="Tax" maxOccurs="unbounded">
                                      <complexType>
                                        <attribute name="Amount" type="string" use="required"/>
                                        <attribute name="Code" type="string" use="required"/>
                                      </complexType>
                                    </element>
                                  </sequence>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="Amount" type="string" use="required"/>
                          </complexType>
                        </element>
                        <element name="SpanishLargeFamilyDiscountLevel" minOccurs="0">
                          <annotation>
                            <documentation>"SpanishLargeFamilyDiscountLevel" is used to specify the Spanish large family discount level. Please note that this qualifier is only applicable to Spain-based subscribers.</documentation>
                          </annotation>
                          <simpleType>
                            <restriction base="string">
                              <enumeration value="1"/>
                              <enumeration value="2"/>
                            </restriction>
                          </simpleType>
                        </element>
                        <element name="SpecificFare" minOccurs="0" maxOccurs="unbounded">
                          <annotation>
                            <documentation>Please note that the element is only applicable to Abacus.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="FareBasis" type="string" minOccurs="0"/>
                            </sequence>
                            <attribute name="RPH" type="string" use="optional">
                              <annotation>
                                <documentation>"RPH" is used to match up with ../OptionalQualifiers/PricingQualifiers/ItineraryOptions/Segment.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="SpecificPenalty" minOccurs="0">
                          <annotation>
                            <documentation>"SpecificPenalty" is used to specify change or cancellation amounts for an itinerary.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="Changeable" minOccurs="0">
                                <annotation>
                                  <documentation>It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Any" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"Any" is used to specify only changeable fare options.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="CurrencyCode" type="string" use="optional">
                                    <annotation>
                                      <documentation>"CurrencyCode" is used to specify the specific currency for maximum exchange penalty.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="MaxPenalty" type="string" use="optional">
                                    <annotation>
                                      <documentation>"MaxPenalty" is used to specify maximum change penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-changeable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="BeforeDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"BeforeDeparture" is used to query for before depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="AfterDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"AfterDeparture" is used to query for after depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element name="EitherOr" minOccurs="0">
                                <annotation>
                                  <documentation>It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Any" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"Any" is used to specify only changeable fare options.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="CurrencyCode" type="string" use="optional">
                                    <annotation>
                                      <documentation>"CurrencyCode" is used to specify the specific currency for maximum exchange penalty.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="MaxPenalty" type="string" use="optional">
                                    <annotation>
                                      <documentation>"MaxPenalty" is used to specify maximum change penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-changeable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="BeforeDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"BeforeDeparture" is used to query for before depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="AfterDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"AfterDeparture" is used to query for after depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element name="Refundable" minOccurs="0">
                                <annotation>
                                  <documentation>It is not possible to combine "BeforeDeparture" and "AfterDeparture" within one element.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Any" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"Any" is used to specify only changeable fare options.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="CurrencyCode" type="string" use="optional">
                                    <annotation>
                                      <documentation>"CurrencyCode" is used to specify the specific currency for maximum exchange penalty.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="MaxPenalty" type="string" use="optional">
                                    <annotation>
                                      <documentation>"MaxPenalty" is used to specify maximum change penalty. If a value of "0" is input no penalty is preferred. If a value of "N" is input only non-changeable options are requested. Please note that monetary amount that includes a decimal is not allowed</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="BeforeDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"BeforeDeparture" is used to query for before depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="AfterDeparture" type="boolean" use="optional">
                                    <annotation>
                                      <documentation>"AfterDeparture" is used to query for after depature fees.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="AdditionalInfo" type="boolean" use="optional">
                              <annotation>
                                <documentation>"AdditionalInfo" is used to receive additional tags with information on fare flexibility.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Taxes" minOccurs="0">
                          <complexType>
                            <sequence>
                              <element name="NoTax" minOccurs="0">
                                <complexType>
                                  <attribute name="Ind" type="boolean" use="optional"/>
                                </complexType>
                              </element>
                              <element name="TaxExempt" minOccurs="0" maxOccurs="5">
                                <complexType>
                                  <attribute name="Code" type="string" use="optional"/>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element name="ValidityDates" minOccurs="0" maxOccurs="unbounded">
                          <annotation>
                            <documentation>This node is used to specify ticket validity dates.Cannot combine with PhaseIV.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="NotValidAfter" type="spct:date" minOccurs="0"/>
                              <element name="NotValidBefore" type="spct:date" minOccurs="0"/>
                              <element name="Segment" maxOccurs="unbounded">
                                <complexType>
                                  <attribute name="EndNumber" type="integer" use="optional"/>
                                  <attribute name="Number" type="integer" use="required"/>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element name="PostProcessing" minOccurs="0">
              <complexType>
                <sequence>
                  <element name="EndTransaction" minOccurs="0">
                    <annotation>
                      <documentation>"EndTransaction" is used to end the transaction and finalize the record.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element name="Source">
                          <complexType>
                            <attribute name="ReceivedFrom" type="string" use="required">
                              <annotation>
                                <documentation>"ReceivedFrom" is used to receive the record.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Email" minOccurs="0">
                          <annotation>
                            <documentation>"Email" is used to send an email notification upon end transaction to any email addresses contained within the particular record.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="eTicket" minOccurs="0">
                                <annotation>
                                  <documentation>"eTicket" is used to send an email notification containing a text-based copy of the eTicket, cannot combine with .../Invoice, or .../Itinerary..</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="PDF" minOccurs="0">
                                      <complexType>
                                        <attribute name="Ind" type="boolean" use="required"/>
                                      </complexType>
                                    </element>
                                  </sequence>
                                  <attribute name="Ind" type="boolean" use="required"/>
                                </complexType>
                              </element>
                              <element name="Invoice" minOccurs="0">
                                <annotation>
                                  <documentation>"Invoice" is used to send an email notification containing a text-based copy of the invoice. Invoice cannot combine with .../eTicket, or .../Itinerary.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Ind" type="boolean" use="required"/>
                                </complexType>
                              </element>
                              <element name="Itinerary" minOccurs="0">
                                <annotation>
                                  <documentation>"Itinerary" is used to send an email notification containing a text-based copy of the itinerary. Itinerary cannot combine with .../eTicket, or .../Invoice.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="PDF" minOccurs="0">
                                      <annotation>
                                        <documentation>"PDF" is used to send an email notification containing a PDF-based copy of the itinerary.</documentation>
                                      </annotation>
                                      <complexType>
                                        <attribute name="Ind" type="boolean" use="required"/>
                                      </complexType>
                                    </element>
                                    <element name="Segment" minOccurs="0" maxOccurs="unbounded">
                                      <complexType>
                                        <attribute name="EndNumber" type="integer" use="optional">
                                          <annotation>
                                            <documentation>"EndNumber" is used to specify an ending segment in the range to include in the email message.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="Number" type="integer" use="required">
                                          <annotation>
                                            <documentation>"Number" is used to specify a segment number to include in the email message.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                  </sequence>
                                  <attribute name="Ind" type="boolean" use="required"/>
                                </complexType>
                              </element>
                              <element name="PersonName" minOccurs="0">
                                <complexType>
                                  <attribute name="NameNumber" type="string" use="required">
                                    <annotation>
                                      <documentation>"NameNumber" is used to specify a particular passenger name number to send the email notification to.  Please note that the email address in the PNR's email field must be name associated to the specified name number.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="Ind" type="boolean" use="required"/>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="acceptNegotiatedFare" type="boolean" use="optional">
                  <annotation>
                    <documentation>"acceptNegotiatedFare" is used to accept a negotiated fare when it is not possible to ticket the stored fare.</documentation>
                  </annotation>
                </attribute>
                <attribute name="acceptPriceChanges" type="boolean" use="optional">
                  <annotation>
                    <documentation>"acceptPriceChanges" is used to specify that system should issue a ticket even if price increases while processing.</documentation>
                  </annotation>
                </attribute>
                <attribute name="actionOnPQExpired" use="optional">
                  <annotation>
                    <documentation>"actionOnPQExpired" is used to specify appropriate action when Price Quote expires. Available options are "R" - reprice, "Q" Quit, "O" Override.</documentation>
                  </annotation>
                  <simpleType>
                    <restriction base="string">
                      <enumeration value="O"/>
                      <enumeration value="Q"/>
                      <enumeration value="R"/>
                    </restriction>
                  </simpleType>
                </attribute>
              </complexType>
            </element>
          </sequence>
          <attribute name="targetCity" type="string" use="optional">
            <annotation>
              <documentation>"TargetCity" is used to specify to which city service should change context using ContextChange (AAA) and the rest of the service uses new security. If empty or equals current city the context doesn't change.</documentation>
            </annotation>
          </attribute>
        </extension>
      </complexContent>
    </complexType>
  </element>
</schema>
